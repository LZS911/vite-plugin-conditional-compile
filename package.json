        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LZS911/vite-plugin-conditional-compile"
  },
  "keywords": [
    "vite",
    "javascript",
    "ifdebug",
    "conditional",
    "compile",
    "vite-plugin",
    "#ifdef",
    "#ifel",
    "#else",
    "#endif"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "yarn test && yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.11.7",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@babel/core": "7.15.8"
  },
  "peerDependencies": {
    "vite": "^4.5.0"
  }
